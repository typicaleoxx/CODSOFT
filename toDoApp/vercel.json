{
    "builds": [{
        "src": "toDoApp/manage.py", 
        "use": "@vercel/python"
    }],
    "routes": [
        {
            "src": "/api/(.*)", 
            "dest": "toDoApp/wsgi.py" 
        },
        {
            "src": "/(.*)",
            "dest": "toDoApp/wsgi.py" 
        }
    ]
}
